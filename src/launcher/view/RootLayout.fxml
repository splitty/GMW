              <MenuItem mnemonicParsing="false" onAction="#handleClose" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Extras">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleImprove" text="Help Improve This Software" />
                  <MenuItem mnemonicParsing="false" onAction="#handleTwitter" text="Dev Twitter" />
              <MenuItem mnemonicParsing="false" onAction="#handleAbout" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
